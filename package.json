  "packageManager": "pnpm@9.0.6+sha256.0624e30eff866cdeb363b15061bdb7fd9425b17bc1bb42c22f5f4efdea21f6b3",
  "keywords": [
    "monorepo",
    "turborepo",
    "typescript",
    "node.js",
    "next.js",
    "firebase"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "clean:root": "rm -r node_modules && rm -r .turbo",
