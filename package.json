  "dependencies": {
    "@yarnpkg/plugin-workspace-tools": "^4.0.0-rc.42",
    "daisyui": "4.6.0",
    "dotenv-cli": "^7.2.1",
    "lodash": "^4.17.21",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.3",
    "@repo/eslint-config": "workspace:*",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^7.32.0",
